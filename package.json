{
  "name": "web-ui",
  "private": true,
  "version": "1.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc -b && vite build",
    "build:dev": "tsc -b && vite build --mode development",
    "build:qa": "tsc -b && vite build --mode qa",
    "build:demo": "tsc -b && vite build --mode demo",
    "build:stg": "tsc -b && vite build --mode staging",
    "build:prod": "tsc -b && vite build --mode production",
    "build:prod:eu": "tsc -b && vite build --mode production-eu",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "husky",
    "version:current": "npx tsx scripts/version.ts current",
    "version:bump": "npx tsx scripts/version.ts bump",
    "version:auto-bump": "npx tsx scripts/version.ts auto-bump",
    "version:tag": "npx tsx scripts/version.ts tag",
    "version:validate": "npx tsx scripts/version.ts validate",
    "version:info": "npx tsx scripts/version.ts info",
    "version:compare": "npx tsx scripts/version.ts compare",
    "version:patch": "npm run version:bump patch",
    "version:minor": "npm run version:bump minor",
    "version:major": "npm run version:bump major",
    "release:prepare": "npm run version:auto-bump && npm run version:tag",
    "release:patch": "npm run version:patch && npm run version:tag",
    "release:minor": "npm run version:minor && npm run version:tag",
    "release:major": "npm run version:major && npm run version:tag"
  },
  "dependencies": {
    "boolean": "^3.2.0",
    "eslint-plugin-prettier": "^5.4.1",
    "node-global-proxy": "^1.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@swc/plugin-emotion": "^10.0.0",
    "@types/logrocket-react": "^3.0.3",
    "@types/node": "^24.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "msw": "^2.10.2",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.3.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "resolutions": {
    "boolean": "^3.2.0"
  }
}
